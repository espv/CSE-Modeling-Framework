Assign 10 => Temp, 1 => Fire
Define Fire(area: string, temp: float)
From   Temp(value>45) and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp and
last   Temp([string] area = Temp.area, value>45)
within 5000 from Temp
Where   area := Temp.area, temp := Temp.value
Consuming Temp;
