            },
            1
          ]
        },
        {"command": "addQueries", "arguments":
          [
            {
              "name": "then_operator_2states",
              "id": 1,
              "number_states": 2,
              "temperature": 45
            },
            1
          ]
        },
        {"command": "processEvents", "arguments": [10000]},
        {"command": "clearQueries"},
        {"command": "addQueries", "arguments":
          [
            {
              "name": "then_operator_2states",
